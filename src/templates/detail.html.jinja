{% extends "base.html" %}
{% block title %}Detail{% endblock %}

{% block content %}

<h2>Details about '{{ id }}'</h2>

<a href="{{ url_for('index', o='table', q=id ) }}">&gt; Filter for key</a>

<pre>
{{ results_yaml }}
</pre>

% if data != {}
<h2>Data</h2>
 {% for id, item in data.items() %}
  <h3>{{id}}</h3>
  <pre>{{ item }}</pre>
 {% endfor %}
%endif

{% endblock content %}
